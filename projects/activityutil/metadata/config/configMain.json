{
    "metaDataSubsetsList": [
        "main"
    ],
    "metaDataComponentTypes": [
        {
            "main": {
                "documents": false,
                "folders": false
            }
        }
    ],
    "metaDataProjectConfig": [
        {
            "main": {
                "projectTypeConfigFile": "admconfig:activityutil"
            }
        }        
    ],
    "metaDataFtpSender": [
        {
            "main": {
                "files": {
                    "f_html": {"local":"mediadir", "type":"zip", "action":[1], "remoteIndex":"index.html"},
                    "f_css": {"local":"mediadir", "type":"css", "action":[0], "remoteDir":"css/"}
                },
                "ftpId": "activity_util"
            }
        }
    ],
    "metaDataExport": [
        {
            "main": {
                "fields": [
                    {"field":"documents", "ext":".zip"}
                ]
            }
        }
    ],
    "metaDataProjectStructure": [
        {
            "main": "meta.mdpr",
            "viewfiles": {"defaultView": "defaultView"},
            "versions": {
                "fields": 1
            },
            "mainType": {
                "type": "object",
                "typeDef": "activityutil"
            },
            "typesDefinition": {
                "entradaExtres": {
                    "type":"object",
                    "keys": {
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu la dada extra que necessiteu"
                        },
                        "tipus": {
                            "type": "string",
                            "mandatory": true,
                            "default": "dada"
                        },
                        "valor": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Escriviu el valor de la dada aquí"
                        },
                        "parametres": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu els paràmetres per aquesta dada"
                        }
                    }                    
                },
                "defExtres": {
                    "type": "objectArray",
                    "typeDef":"entradaExtres"
                },                                                
                "entradaDocuments": {
                    "type":"object",
                    "keys": {
                        "id": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_SEQ(0)_#}"
                        },
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu el nom del document"
                        },
                        "descripcio": {
                            "type": "string",
                            "default": "Escriu una descripció pel document"
                        },
                        "sendftp": {
                            "type": "boolean",
                            "mandatory": true,
                            "default": false
                        }
                    }                    
                },
                "defDocuments": {
                    "type": "objectArray",
                    "typeDef":"entradaDocuments"
                },                                                
                "activityutil": {
                    "type": "object",
                    "keys": {
                        "autor": {
                            "type": "string",
                            "mandatory": true,
                            "isRole":true
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true
                        },                                                
                        "dadesExtres": {
                            "type": "defExtres",
                            "mandatory": true
                        },
                        "estil": {
                            "type": "string",
                            "mandatory": true,
                            "default": "boostioc"
                        },                                                
                       "entitatResponsable": {
                            "type": "string"
                        },
                        "mostrarAutor": {
                            "type": "boolean",
                            "mandatory": true,
                            "default":true
                        },
                        "documents": {
                            "type": "defDocuments"
                        },
                        "nom_real": {
                            "type": "string",
                            "mandatory": true,
                            "calculateOnSave": {
                                "class": "CalculateRealNameFromUsername",
                                "data": "autor"
                            }
                        },
                        "data_fitxercontinguts": {
                            "type": "string",
                            "mandatory": true,
                            "calculateOnRead": {
                                "class": "CalculateMaxPageDateFromNs",
                                "data": ""
                            }
                        }
                    }
                }
            }
        }
    ]
}
