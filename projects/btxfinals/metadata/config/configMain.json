                    }
                },
                "defNomsReals": {
                    "type": "objectArray",
                    "typeDef": "llistaNomsReals"
                },
                "btxfinals": {
                    "type": "object",
                    "keys": {
                        "titol": {
                            "type": "string",
                            "mandatory": true,
                            "default": "ordinària"
                        },
                        "subtitol": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Horaris i centres d'exàmens presencials"
                        },
                        "quadrimestre": {
                            "type": "number",
                            "mandatory": true,
                            "default": 1
                        },
                        "curs": {
                            "type": "string",
                            "mandatory": true,
                            "default": "2024-2025"
                        },
                        "dadesProves": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Els exàmens presencials són de les matèries comunes de 2n curs."
                        },
                        "estil": {
                            "type": "string",
                            "mandatory": true,
                            "default": "main"
                        },
                        "autor": {
                            "type": "string",
                            "mandatory": true,
                            "isRole":true
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true
                        },                                                
                        "dadesCentres": {
                            "type": "defCentres",
                            "mandatory": true
                        },
                        "labelData1": {
                            "type": "string",
                            "struc_chars": "30",
                            "mandatory": true,
                            "default":""
                        },
                        "data1": {
                            "type": "date",
                            "mandatory": true,
                            "default":"01/01/2000"
                        },
                        "activaData2": {
                            "type": "boolean",
                            "mandatory": false,
                            "default":false
                        },
                        "labelData2": {
                            "type": "string",
                            "struc_chars": "30",
                            "mandatory": true,
                            "default":""
                        },                                                
                        "data2": {
                            "type": "date",
                            "default":"01/01/2000"
                        },
                        "labelData3": {
                            "type": "string",
                            "struc_chars": "30",
                            "mandatory": true,
                            "default":""
                        },
                        "activaData3": {
                            "type": "boolean",
                            "mandatory": false,
                            "default":false
                        },                                                
                        "data3": {
                            "type": "date",
                            "default":"01/01/2000"
                        },
                        "nom_real": {
                            "type": "defNomsReals",
                            "mandatory": true,
                            "calculateOnSave": {
                                "class": "CalculateRealNameFromUsername",
                                "data": "autor"
                            }
                        }
                    }
                }
            }
        }
