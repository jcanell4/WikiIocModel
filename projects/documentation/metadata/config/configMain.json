{
    "metaDataClassesNameSpaces": [
        {
            "main": {
                "MetaDataDAO": "defaultProject",
                "MetaDataEntity": "defaultProject",
                "MetaDataRender": "documentation"
            }
        }
    ],
    "metaDataProjectStructure": [
        {
            "main": "meta.mdpr",
            "mainType": {
                "type": "object",
                "typeDef": "document"
            },
            "typesDefinition": {
                "document": {
                    "type": "object",
                    "keys": {
                        "autor": {
                            "type": "string",
                            "mandatory": true
                        },
                        "descripcio": {
                            "type": "string",
                            "default": "write a brief project description here"
                        },
                        "nsproject": {
                            "type": "string",
                            "mandatory": true
                        },
                        "plantilla": {
                            "type": "string",
                            "mandatory": true,
                            "default": "plantilles:projects:continguts"
                        },
                        "fitxercontinguts": {
                            "type": "string",
                            "mandatory": true
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "struc_rows": "1",
                            "mandatory": true
                        },
                        "titol": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Dades generals del projecte"
                        },
                        "media_address": {
                            "type": "string",
                            "default": "../media"
                        }
                    }
                }
            }
        }
    ]
}
