{
    "reversion": {
        "class": "WikiIocButton",
        "parms": { 
            "DOM": {
               "id": "reversion",
               "title": "Revertir",
               "class": "iocDisplayBlock"
            },
            "DJO": {
               "query": "'do=revert'",
               "autoSize": true,
               "visible": false,
               "iconClass": "'iocIconUndo'",
               "urlBase": "'lib/exe/ioc_ajax.php?call=projectRevert'",
               "standbyId": "'bodyContent'",
               "disableOnSend": true
            }
        },

        "scripts": {
            "getFunctions": {
                "path": "revertGetFunction.js"
            },
            "updateHandler": {
                "permissions": [
                   "isadmin",
                   "ismanager",
                   "isprojectmanager"
                ],
                "rols": [
                   "responsable"
                ],
                "conditions": {
                    "page.action":"'project_view'"
                }
            }
        }

    },
