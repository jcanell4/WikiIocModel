{
    "metaDataSubsetsList": [
        "main"
    ],
    "metaDataComponentTypes": [
        {
            "main": {
                "documents": false,
                "folders": false
            }
        }
    ],
    "metaDataProjectConfig": [
        {
            "main": {
                "projectTypeConfigFile": "admconfig:manual"
            }
        }        
    ],
    "metaDataFtpSender": [
        {
            "main": {
                "files": {
                    "Floe1": {"local":"mediadir", "type":"zip", "action":[1], "remoteIndex":"index.html"}
                },
                "ftpId": "manuals"
            }
        }
    ],
    "metaDataProjectStructure": [
        {
            "main": "meta.mdpr",
            "viewfiles": {"defaultView": "defaultView"},
            "versions": {
                "fields": 2
            },
            "mainType": {
                "type": "object",
                "typeDef": "manual"
            },
            "typesDefinition": {
                "entradaExtres": {
                    "type":"object",
                    "keys": {
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu la dada extra que necessiteu"
                        },
                        "tipus": {
                            "type": "string",
                            "mandatory": true,
                            "default": "dada"
                        },
                        "valor": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Escriviu el valor de la dada aquí"
                        },
                        "parametres": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu els paràmetres per els tipus que ho necessitin"
                        }
                    }                    
                },
                "defExtres": {
                    "type": "objectArray",
                    "typeDef":"entradaExtres"
                },                                                
                "entradaDocuments": {
                    "type":"object",
                    "keys": {
                        "id": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_SEQ(0)_#}"
                        },
                        "ordre": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu el nom del document"
                        },
                        "descripcio": {
                            "type": "string",
                            "default": "Escriu una descripció pel document"
                        }
                    }                    
                },
                "defDocuments": {
                    "type": "objectArray",
                    "typeDef":"entradaDocuments"
                },                                                
                "manual": {
                    "type": "object",
                    "keys": {
                        "titol": {
                            "type": "string",
                            "mandatory": true,
                            "default":"Poseu-hi el títol"
                        },
                        "subtitol": {
                            "type": "string",
                            "default": "Indiqueu el subtítol",
                            "mandatory": true
                        },
                        "autor": {
                            "type": "string",
                            "mandatory": true,
                            "isRole":true
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true
                        },                                                
                        "dadesExtres": {
                            "type": "defExtres",
                            "mandatory": true
                        },
                        "estil": {
                            "type": "string",
                            "mandatory": true,
                            "default": "main"
                        },                                                
                        "nivells": {
                            "type": "string",
                            "mandatory": true,
                            "default": "3"
                        },                                                
                        "entitatResponsable": {
                            "type": "string"
                        },
                        "mostrarAutor": {
                            "type": "boolean",
                            "mandatory": true,
                            "default":true
                        },
                        "amagarMenuInici": {
                            "type": "boolean",
                            "mandatory": true,
                            "default":false
                        },
                        "documents": {
                            "type": "defDocuments",
                            "default": [{"id":0, "ordre":1, "nom":"continguts", "descripcio":"descripció del document"}]
                        },
                        "nom_real": {
                            "type": "string",
                            "mandatory": true,
                            "calculateOnSave": {
                                "class": "CalculateRealNameFromUsername",
                                "data": "autor"
                            }
                        },
                        "data_fitxercontinguts": {
                            "type": "string",
                            "mandatory": true,
                            "calculateOnRead": {
                                "class": "CalculateMaxPageDateFromNs",
                                "data": ""
                            }
                        }
                    }
                }
            }
        }
    ]
}
