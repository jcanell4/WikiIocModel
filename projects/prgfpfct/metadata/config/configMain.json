                            "type": "objectArray",
                            "typeDef":"entradaCc_historic",
                            "mandatory": true,
                            "props": {
                                "readonly": true
                            }
                        },
                        "cc_raonsModificacio": {
                            "type": "string",
                            "mandatory": true
                        },
                        "cc_dadesAutor": {
                            "type": "object",
                            "typeDef": "dadesGestioDocument",
                            "mandatory": true
                        },
                        "cc_dadesRevisor": {
                            "type": "object",
                            "typeDef": "dadesGestioDocument",
                            "mandatory": true
                        },
                        "cc_dadesValidador": {
                            "type": "object",
                            "typeDef": "dadesGestioDocument",
                            "mandatory": true
                        },
                        "autor": {
                            "type": "string",
                            "mandatory": true,
                            "isRole":true,
                            "roleProperties":{
                                "wiki_permission":16,
                                "shortcut":true
                            }
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true,
                            "roleProperties":{
                                "wiki_permission":16,
                                "shortcut":true
                            }
                        },
                        "revisor": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true,
                            "roleProperties":{
                                "wiki_permission":4,
                                "shortcut":false,
                                "order":2
                            }
                        },
                        "validador": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true,
                            "roleProperties":{
                                "wiki_permission":4,
                                "shortcut":false,
                                "order":1
                            }
                        },
                        "documentVersion": {
                            "type": "number",
                            "calculateOnRead": {
                                "class": "CalculateFromConfigMainSimpleValue",
                                "data": {
                                    "key": "metaDataConstants",
                                    "subset": "main",
                                    "path": ["documentVersion", "value"]
                                }
                            }
                        }

                    }
                }
            }
        },
        {
            "management": "management.mdpr",
            "versions": {
                "fields": 0
            },
            "mainType": {
                "type": "object",
                "typeDef": "management"
            },
            "typesDefinition": {
                "wfStateHistoryEntry":{
                    "type": "object",
                    "keys": {
                        "changeDate":{
                            "type": "date",
                            "mandatory": true
                        },
                        "oldState":{
                            "type": "string",
                            "mandatory": true
                        },
                        "newState":{
                            "type": "string",
                            "mandatory": true
                        },
                        "changeAction":{
                            "type": "string",
                            "mandatory": true
                        },
                        "user":{
                            "type": "string",
                            "mandatory": true
                        }
                    }
                },
                "workflowType":{
                    "type": "object",
                    "keys": {
                        "currentState":{
                            "type": "string",
                            "mandatory": true,
                            "default": "undefined"
                        },
                        "stateHistory":{
                            "type": "objectArray",
                            "typeDef":"wfStateHistoryEntry"
                        }
                    }
                },
                "management": {
                    "type": "object",
                    "keys": {
                        "workflow": {
                            "type": "object",
                            "typeDef": "workflowType",
                            "mandatory": true
                        }
                    }
                }
            }
        }
