                "fields": 0,
                "templates": {"continguts":0}
            },
            "mainType": {
                "type": "object",
                "typeDef": "programacio"
            },
            "typesDefinition": {
                "entradaResultatsAprenentatgeObjectiusTerminals": {
                    "type":"object",
                    "keys": {
                        "ra": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "descripcio": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Escriviu el contingut del Resultat d'Aprenentatge (LOE) o de l'Objectiu Terminal (LOGSE)"
                        }
                    }
                },
                "defResultatsAprenentatgeObjectiusTerminals": {
                    "type": "objectArray",
                    "typeDef":"entradaResultatsAprenentatgeObjectiusTerminals"
                },
                "entradaCriterisAvaluacio": {
                    "type":"object",
                    "keys": {
                        "ra": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_COPY(1)_#}"
                        },
                        "ca": {
                            "type": "string",
                            "mandatory": true,
                            "default": "1.{#_INC(1)_#}"
                        },
                        "descripcio": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Escriviu el contingut del Criteri d'Avaluacio (LOE)"
                        }
                    }
                },
                "defCriterisAvaluacio": {
                    "type": "objectArray",
                    "typeDef":"entradaCriterisAvaluacio"
                },
                "entradaCc_historic":{
                    "type":"object",
                    "keys":{
                        "data":{
                            "type": "date",
                           "mandatory": true,
                           "props": {
                               "readonly": true
                            }
                        },
                        "autor": {
                            "type": "string",
                            "mandatory": true,
                            "props": {
                                "readonly": true
                            }
                        },
                        "modificacions": {
                            "type": "string",
                            "mandatory": true,
                            "props": {
                                "readonly": true
                            }
                        }
                    }
                },
                "dadesGestioDocument":{
                    "type":"object",
                    "keys":{
                        "nomGestor": {
                            "type": "string",
                            "mandatory": true,
                            "props": {
                                "readonly": true
                            }
                        },
                        "carrec": {
                            "type": "string",
                            "mandatory": true
                        },
                        "dataDeLaGestio": {
                            "type": "date",
                            "mandatory": true,
                            "props": {
                                "readonly": true
                            }
                        },
                        "signatura": {
                            "type": "string",
                            "mandatory": true,
                            "default":"pendent",
                            "props": {
                                "readonly": true
                            }
                        }
                    }
                },
                "programacio": {
                    "type": "object",
                    "keys": {
                        "departament": {
                            "type": "string",
                            "mandatory": true,
                            "default":"Indiqueu de quin departament és la programació."
                        },
                        "tipusCicle": {
                            "type": "string",
                            "mandatory": true,
                            "default": "LOE"
                        },
                        "cicle": {
                            "type": "string",
                            "default": "Indiqueu el nom del cicle",
                            "mandatory": true
                        },
                        "modulId": {
                            "type": "number",
                            "mandatory": true
                        },
                        "durada": {
                            "type": "string",
                            "mandatory": true,
                            "default": 0
                        },
                        "plantilla": {
                            "type": "string",
