{
    "metaDataClassesNameSpaces": [
        {
            "main": {
                "MetaDataDAO": "defaultProject",
                "MetaDataEntity": "defaultProject",
                "MetaDataRender": "documentation"
            }
        }
    ],
    "metaDataProjectStructure": [
        {
            "main": "meta.mdpr",
            "mainType": {
                "type": "object",
                "typeDef": "material"
            },
            "typesDefinition": {
                "material": {
                    "type": "object",
                    "keys": {
                        "autor": {
                            "type": "string",
                            "mandatory": true
                        },
                        "blocs": {
                            "type": "array",
                            "mandatory": false,
                            "itemsType": "bloc"
                        },
                        "descripcio": {
                            "type": "string",
                            "default": "write a brief project description here"
                        },
                        "nsproject": {
                            "type": "string",
                            "mandatory": true
                        },
                        "plantilla": {
                            "type": "string",
                            "mandatory": true,
                            "default": "plantilles:projects:continguts"
                        },
                        "fitxercontinguts": {
                            "type": "string",
                            "mandatory": true
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "struc_rows": "1",
                            "mandatory": true
                        },
                        "titol": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Dades generals del projecte"
                        },
                        "unitat":{
                            "type": "object",
                            "typeDef": "unitat"
                        }
                    }
                },
                "bloc": {
                    "type": "array",
                    "itemsType": "unitat"
                },
                "unitat": {
                    "type": "object",
                    "keys": {
                        "title": {
                            "type": "string",
                            "mandatory": false
                        },
                        "autor": {
                            "type": "string",
                            "mandatory": false
                        },
                        "apartats": {
                            "type": "array",
                            "mandatory": true,
                            "itemsType": "apartat"
                        }
                    }
                },
                "apartat": {
                    "type": "object",
                    "keys": {
                        "title": {
                            "type": "string",
                            "mandatory": false
                        },
                        "autor": {
                            "type": "string",
                            "mandatory": false,
                            "inherit": true
                        }
                    }
                }
            }
        }
    ]
}
