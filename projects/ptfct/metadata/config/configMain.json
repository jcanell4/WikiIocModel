        {
            "main": {
                "MetaDataDAO": "defaultProject",
                "MetaDataEntity": "defaultProject",
                "MetaDataRender": "ptfct"
            }
        }
    ],
    "metaDataProjectConfig": [
        {
            "main": {
                "projectTypeConfigFile": "admconfig:fct"
            }
        }
    ],
    "metaDataFtpSender": [
        {
            "main": {
                "files": {
                    "F1": {"local":"mediadir", "type":"zip", "action":[1], "remoteIndex":"index.html"}
                },
                "ftpId": "pla_treball"
            }
        }
    ],
    "metaDataProjectStructure": [
        {
            "main": "meta.mdpr",
            "versions": {
                "fields": 1,
                "templates": {
                    "continguts": 2
                }
            },
            "mainType": {
                "type": "object",
                "typeDef": "pla"
            },
            "typesDefinition": {
                "pla": {
                    "type": "object",
                    "keys": {
                        "semestre": {
                            "type": "number",
                            "mandatory": true,
                            "default": 1
                        },
                        "tipusCicle": {
                            "type": "string",
                            "mandatory": true,
                            "default": "LOE"
                        },
                        "cicle": {
                            "type": "string",
                            "default": "Indiqueu el nom del cicle",
                            "mandatory": true
                        },
                        "durada": {
                            "type": "string",
                            "mandatory": true,
                            "default": 0,
                            "props": {
                                "readonly": true
                            }
                        },
                        "any": {
                            "type": "number",
                            "mandatory": true,
                            "calculatedDefault": {
                                "class": "CalculateByCurrentDate",
                                "data": "Y"
                            }
                        },
                        "dataIniciFCT": {
                            "type": "date",
                            "mandatory": true,
                            "default": "01/01/2000"
                        },
                        "dataMaxIniciFCT": {
                            "type": "date",
                            "mandatory": true,
                            "default": "01/01/2000"
                        },
                        "dataApteFCT": {
                            "type": "date",
                            "mandatory": true,
                            "default": "01/01/2000"
                        },
                        "dataMaxApteFCT": {
                            "type": "date",
                            "mandatory": true,
                            "default": "01/01/2000"
                        },
                        "autor": {
                            "type": "string",
                            "mandatory": true,
                            "isRole":true
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true
                        },
                        "supervisor": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true
                        },
                        "coordinador": {
                            "type": "string",
                            "mandatory": true
                        },
                        "fitxercontinguts": {
                            "type": "string",
                            "mandatory": true,
                            "default": "continguts"
                        },
                        "moodleCourseId": {
                            "type": "number",
                            "mandatory": true,
                            "default": 0
                        }
                    }
                }
            }
        }
    ]
}
