                                        "unitat",
                                        "nom",
                                        "ordreImparticio",
                                        "hores",
                                        "ponderació"
                                    ],
                                    "defaultValue":{
                                        "_type_":"field",
                                        "name":"taulaDadesUn",
                                        "values": "$values",
                                        "defaultValue": "[]"
                                    }
                                }
                            },
                            "props" : {
                                "readonly": {"class":"ValidateIfExistFieldAndHasValue", "data":{"responses":["nsProgramacio"]}}          
                            }
