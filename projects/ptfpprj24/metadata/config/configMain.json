                        },
                        "taulaDadesUn": {
                            "type": "defTaulaDadesUn",
                            "mandatory": true,
                            "calculateOnRead": {
                                "class": "CalculateArrayObjectFromExternalField",
                                "data": {
                                    "projectId": {
                                        "_type_": "field",
                                        "name": "nsProgramacio",
                                        "values":"$values",
                                        "defaultValue":""
                                    },
                                    "metaDataSubSet": "main",
                                    "field": "taulaDadesUn",
