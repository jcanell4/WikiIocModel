                            "default":"{#_TODAY()_#}"
                        },    
                        "dataRecuQualificacioSem2": {
                            "type": "date",
                            "mandatory": true,
                            "default":"{#_TODAY()_#}"
                        },                                                                                                                                                                    
                        "dadesExtres": {
                            "type": "defExtres",
                            "mandatory": true,
                            "calculateOnRead": {
                                "class": "CalculateDateFormatFromArrayObject",
                                "data": {
                                    "field": "dadesExtres",
                                    "conditionColumn": "tipus",
                                    "conditionValue": "data_actualitzable",
                                    "formatField": "valor",
                                    "format": "DDMMYYYY",
                                    "separator": "/", 
                                    "defaultValue": "[]"
                                }
                            },
                            "calculateOnSave": {
                                "class": "CalculateDateFormatFromArrayObject",
                                "data": {
                                    "field": "dadesExtres",
                                    "conditionColumn": "tipus",
                                    "conditionValue": "data_actualitzable",
